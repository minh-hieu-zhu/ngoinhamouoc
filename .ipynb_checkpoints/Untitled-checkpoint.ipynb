   "source": [
    "import turtle\n",
    "\n",
    "t = turtle.Turtle()\n",
    "\n",
    "t.speed(10)\n",
    "turtle.bgcolor('light blue');\n",
    "for i in range(2):\n",
    "    t.fd(200)\n",
    "    t.rt(90)\n",
    "    t.fd(100)\n",
    "    t.rt(90)\n",
    "t.lt(45)\n",
    "t.fd(80)\n",
    "t.rt(90)\n",
    "t.fd(80)\n",
    "t.penup()\n",
    "t.lt(180)\n",
    "t.fd(20)\n",
    "t.pendown()\n",
    "t.rt(45)\n",
    "t.fd(40)"
   ]
  }
